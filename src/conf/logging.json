{
    "_description": "Logging configuration",
    "_author": "jan",
    "_version": "1.0",
    "__root__": {
    	"level": "debug", "destination": "file", "append": false
    },
    "__main__": { "destination": "stderr" },
  
    "sweattrails.device.ant": { "flat": true, "destination": "stderr", "level": "info" },
    "__sweattrails.device.fitparse": { "flat": true, "destination": "file" },
    "sweattrails": { "destination": "stderr", "flat": true },
  
    "gripe": { "flat": true },
    "gripe.db": { "flat": true },
    
    "grudge": { "flat": true },
    "grit": { "flat": true },
    "grizzle": { "flat": true },

    "grumble": { "flat": true },
    "grumble.qt": { "flat": true },
    "grumble.qt.bridge": { "flat": true, "destination": "stderr" }
}
